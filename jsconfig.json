{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*": ["*", "scripts/*", "updatingFeatureLayer/*"]
    },
    "checkJs": true,
    "allowJs": true,
    "strict": false,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["scripts/**/*", "updatingFeatureLayer/**/*"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
